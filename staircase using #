import math
import os
import random
import re
import sys

def staircase(n):
    k = n-1
    for i in range(n):
        for j in range(k):
            print(end=" ")
        k = k-1
        for j in range(i+1):
            print("#",end="")
        print("")
if __name__ == '__main__':
    n = int(input())

    staircase(n)
